{
  "name": "railgun-contracts",
  "version": "0.0.1",
  "description": "Railgun Contract",
  "license": "UNLICENSED",
  "scripts": {
    "lint": "solhint 'contracts/**/*.sol'; prettier --check './**'; eslint .",
    "lint:fix": "npx prettier --write './**'; npx solhint --fix 'contracts/**/*.sol'",
    "clean": "hardhat clean",
    "compile": "npm run clean && npm run lint && hardhat compile",
    "test": "hardhat test",
    "test:extra": "hardhat test --longtests extra",
    "test:complete": "hardhat test --longtests complete",
    "analyze": "npm run clean && npm run lint && ./slither.sh"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.0",
    "@noble/ed25519": "^1.7.0",
    "@noble/hashes": "^1.1.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.5",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "chai": "^4.3.6",
    "circomlibjs": "^0.1.7",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-tsdoc": "^0.2.16",
    "ethers": "^5.7.0",
    "hardhat": "^2.10.2",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-local-networks-config-plugin": "^0.0.6",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.21",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.7.3",
    "@openzeppelin/contracts-upgradeable": "^4.7.3"
  }
